{
	"name" : "raxon/framework",
	"description" : "raxon/framework see https://raxon.org for detailed usage",
	"type" : "library",
	"keywords" : ["framework", "raxon", "raxon.org", "org","library", "language", "template", "engine"],
	"homepage": "https://raxon.org",
	"require" : {
		"php": ">=8.0",
		"ext-mbstring": "*",
		"ext-json": "*",
		"ext-pdo": "*",
		"ext-posix": "*",
		"ext-zend-opcache": "*",
		"ext-openssl": "*",
		"ext-readline": "*",
		"ext-pcntl": "*",
		"ext-zlib": "*",
		"ext-zip": "*",
		"ext-shmop": "*",
		"raxon/autoload": ">=2026.01.01",
		"raxon/boot": ">=2026.01.01",
		"raxon/basic": ">=2026.01.01",
		"raxon/event": ">=2026.01.01",
		"raxon/config": ">=2026.01.01",
		"raxon/host": ">=2026.01.01",
		"raxon/middleware": ">=2026.01.01",
		"raxon/node": ">=2026.01.01",
		"raxon/output_filter": ">=2026.01.01",
		"raxon/log": ">=2026.01.01",
		"raxon/route": ">=2026.01.01",
		"raxon/server": ">=2026.01.01",
		"raxon/account": ">=2026.01.01",
		"monolog/monolog" : "^3.6",
		"defuse/php-encryption": "^2.3",
		"spatie/fork": "^1.2.2"
    },
	"require-dev" : {
		"pestphp/pest": "^2.34"
	},
	"config": {
		"allow-plugins": {
			"pestphp/pest-plugin": true
		}
	},
	"license" : "MIT",
	"authors" : [{
			"name" : "Remco van der Velde",
			"email" : "development@universeorange.com"
		}
	],
	"autoload" : {
		"psr-4" : {
			"Raxon\\" : "src/"
		}
	}
}